<div class="untree_co-section">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-md-8 col-lg-8 pb-4">
        <form href="/contact" method="POST">  
          <div class="form-group">
            <label class="text-black">Rate  <span class="text-danger">*</span></label>
            <select class="form-control" name="rate">
              <option name="rate" value="1">1</option>
              <option name="rate" value="2">2</option>
              <option name="rate" value="3">3</option>
              <option name="rate" value="4">4</option>
              <option name="rate" value="5">5</option>
            </select>
            <!-- <input type="text" class="form-control" name="rate"> -->
          </div>

          <div class="form-group mb-5">
            <label class="text-black">Comment</label>
            <textarea cols="30" rows="5" class="form-control" name="comment"></textarea>
          </div>

          <button type="submit" class="btn btn-primary-hover-outline">Send</button>
        </form>
      </div>
    </div>
  </div>
</div>